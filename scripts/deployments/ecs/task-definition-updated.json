{
  "family": "prefect-fargate-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "4096",
  "executionRoleArn": "arn:aws:iam::677276117552:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "dummy-container",
      "image": "677276117552.dkr.ecr.eu-central-1.amazonaws.com/dummy-prefect-flow:latest",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/prefect-fargate-task",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "prefect"
        }
      }
    }
  ]
}
